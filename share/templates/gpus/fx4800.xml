<?xml version="1.0" ?>

<gpu 
 xmlns="http://www.hp.com"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.hp.com /opt/vizstack/share/schema/gpuconfig.xsd">

		<model>Quadro FX 4800</model>
		<vendor>NVIDIA Corporation</vendor>
		<scanout_caps>
			<index>0</index>
			<type>digital</type>
			<type>analog</type>
		</scanout_caps>
		<scanout_caps>
			<index>1</index>
			<type>digital</type> <!-- NOTE: the 4800 has only one digital-I port, so only one analog device is supported on Port 0 -->
		</scanout_caps>
		<scanout_caps>
			<index>2</index>
			<type>digital</type> <!-- NOTE: the 4800 has only one digital-I port, so only one analog device is supported on Port 0 -->
		</scanout_caps>
		<limits>
			<max_width>8192</max_width>
			<max_height>8192</max_height>
		</limits>

</gpu>
