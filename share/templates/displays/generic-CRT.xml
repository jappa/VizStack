<?xml version="1.0" ?>

<display
 xmlns="http://www.hp.com"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.hp.com /opt/vizstack/share/schema/displayconfig.xsd" >
	<model>Generic CRT Monitor</model>
	<input>analog</input>
	<hsync>
		<min>30</min>
		<max>150</max>
	</hsync>
	<vrefresh>
		<min>50</min>
		<max>120</max>
	</vrefresh>
	<default_mode>1280x1024_60</default_mode>
	<!-- All modelines generated by GTF. !-->
	<mode>
		<type>modeline</type>
		<alias>1280x1024_60</alias>
		<width>1280</width>
		<height>1024</height>
		<refresh>60</refresh>
		<value>108.88  1280 1360 1496 1712  1024 1025 1028 1060  -HSync +Vsync</value> <!-- 1280x1024 @ 60.00 Hz (GTF) hsync: 63.60 kHz; pclk: 108.88 MHz -->
	</mode>
	<mode>
		<type>modeline</type>
		<alias>640x480_60</alias>
		<width>640</width>
		<height>480</height>
		<refresh>60</refresh>
		<value>23.86  640 656 720 800  480 481 484 497  -HSync +Vsync</value> <!-- 640x480 @ 60.00 Hz (GTF) hsync: 29.82 kHz; pclk: 23.86 MHz -->
	</mode>
	<mode>
		<type>modeline</type>
		<alias>800x600_60</alias>
		<width>800</width>
		<height>600</height>
		<refresh>60</refresh>
		<value>38.22  800 832 912 1024  600 601 604 622  -HSync +Vsync</value> <!-- 800x600 @ 60.00 Hz (GTF) hsync: 37.32 kHz; pclk: 38.22 MHz -->
	</mode>
	<mode>
		<type>modeline</type>
		<alias>1024x768_60</alias>
		<width>1024</width>
		<height>768</height>
		<refresh>60</refresh>
		<value>64.11  1024 1080 1184 1344  768 769 772 795  -HSync +Vsync</value> <!-- 1024x768 @ 60.00 Hz (GTF) hsync: 47.70 kHz; pclk: 64.11 MHz  -->
	</mode>
	<mode>
		<type>modeline</type>
		<alias>1600x1200_60</alias>
		<width>1600</width>
		<height>1200</height>
		<refresh>60</refresh>
		<value>160.96  1600 1704 1880 2160  1200 1201 1204 1242  -HSync +Vsync</value> <!-- 1600x1200 @ 60.00 Hz (GTF) hsync: 74.52 kHz; pclk: 160.96 MHz -->
	</mode>
	<mode>
		<type>modeline</type>
		<alias>1152x864_60</alias>
		<width>1152</width>
		<height>864</height>
		<refresh>60</refresh>
		<value>81.62  1152 1216 1336 1520  864 865 868 895  -HSync +Vsync</value> <!-- 1152x864 @ 60.00 Hz (GTF) hsync: 53.70 kHz; pclk: 81.62 MHz  -->
	</mode>
</display>
