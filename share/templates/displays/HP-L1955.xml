<?xml version="1.0" ?>

<!--

L1955

19" TFT monitor from HP.

-->

<display
 xmlns="http://www.hp.com"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.hp.com /opt/vizstack/share/schema/displayconfig.xsd" >
	<model>HP L1955</model>
	<input>analog</input> <!-- The L1955 has a DVI-I and a VGA input. It ships with a VGA cable only, and that's how many end up using it. Hence this choice ! -->
	<edid>/opt/vizstack/share/templates/displays/edids/HP-L1955.bin</edid>
	<edid_name>HP L1955</edid_name>
	<default_mode>1280x1024_60</default_mode>
	<mode>
		<type>edid</type>
		<alias>1280x1024_60</alias>
		<alias>1280x1024</alias> <!-- default refresh rate 60 Hz -->
		<width>1280</width>
		<height>1024</height>
		<refresh>60</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>1280x1024_75</alias>
		<width>1280</width>
		<height>1024</height>
		<refresh>75</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>1152x864_75</alias>
		<width>1152</width>
		<height>864</height>
		<refresh>75</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>1024x768_75</alias>
		<width>1024</width>
		<height>768</height>
		<refresh>75</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>1024x768_70</alias>
		<width>1024</width>
		<height>768</height>
		<refresh>70</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>1024x768_60</alias>
		<alias>1024x768</alias>
		<width>1024</width>
		<height>768</height>
		<refresh>60</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>800x600_75</alias>
		<width>800</width>
		<height>600</height>
		<refresh>75</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>800x600_72</alias>
		<width>800</width>
		<height>600</height>
		<refresh>72</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>800x600_60</alias>
		<alias>800x600</alias>
		<width>800</width>
		<height>600</height>
		<refresh>60</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>640x480_75</alias>
		<width>640</width>
		<height>480</height>
		<refresh>75</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>640x480_72</alias>
		<width>640</width>
		<height>480</height>
		<refresh>72</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>640x480_60</alias>
		<width>640</width>
		<height>480</height>
		<refresh>60</refresh>
	</mode>
</display>
